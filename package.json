{
  "name": "mantine-storybook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build && node -e \"const fs=require('fs');const f='storybook-static/index.html';let c=fs.readFileSync(f,'utf8');c=c.replace(/__STORY_UI_EDGE_URL__/g,process.env.VITE_STORY_UI_EDGE_URL||'');fs.writeFileSync(f,c);\"",
    "story-ui": "story-ui start --port 4001",
    "storybook-with-ui": "concurrently \"npm run storybook\" \"npm run story-ui\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@mantine/core": "^8.2.5",
    "@mantine/hooks": "^8.2.5",
    "@tabler/icons-react": "^3.34.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@storybook/react-vite": "^9.1.3",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.3",
    "globals": "^16.3.0",
    "storybook": "^9.1.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "@tpitre/story-ui": "latest"
  }
}